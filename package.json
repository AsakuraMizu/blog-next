{
  "name": "sveltekit-template",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,cjs,ts,html,css,svelte,md,json}"
  },
  "devDependencies": {
    "@iconify-json/icon-park-outline": "^1.1.12",
    "@iconify-json/simple-icons": "^1.1.74",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.25.2",
    "@sveltejs/package": "^2.2.2",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "@unocss/core": "^0.56.5",
    "@unocss/eslint-config": "^0.56.5",
    "@unocss/extractor-svelte": "^0.56.5",
    "@unocss/preset-attributify": "^0.56.5",
    "@unocss/preset-icons": "^0.56.5",
    "@unocss/preset-uno": "^0.56.5",
    "@unocss/reset": "^0.56.5",
    "@unocss/transformer-directives": "^0.56.5",
    "@unocss/transformer-variant-group": "^0.56.5",
    "@unocss/vite": "^0.56.5",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-svelte": "^2.34.0",
    "hastscript": "^8.0.0",
    "jsdom": "^22.1.0",
    "lightningcss": "^1.22.0",
    "mdsvex": "^0.11.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "rehype-autolink-headings": "^7.0.0",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-emoji": "^4.0.0",
    "remark-gemoji": "^8.0.0",
    "remark-math": "^6.0.0",
    "remark-toc": "^9.0.0",
    "svelte": "^4.2.1",
    "svelte-check": "^3.5.2",
    "svelte-eslint-parser": "^0.33.1",
    "svelte-preprocess-import-assets": "^1.0.1",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "vite": "^4.4.11",
    "vite-imagetools": "^5.0.9",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@fontsource/noto-sans-sc": "^5.0.14",
    "@fontsource/rubik": "^5.0.15",
    "dayjs": "^1.11.10",
    "shiki": "^0.14.5"
  }
}
