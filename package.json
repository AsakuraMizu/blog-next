{
  "name": "sveltekit-template",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,cjs,ts,html,css,svelte,md,json}"
  },
  "devDependencies": {
    "@iconify-json/icon-park-outline": "^1.1.12",
    "@iconify-json/simple-icons": "^1.1.65",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/kit": "^1.22.5",
    "@sveltejs/package": "^2.2.1",
    "@trivago/prettier-plugin-sort-imports": "^4.2.0",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "@unocss/core": "^0.55.0",
    "@unocss/eslint-config": "^0.55.0",
    "@unocss/extractor-svelte": "^0.55.0",
    "@unocss/preset-attributify": "^0.55.0",
    "@unocss/preset-icons": "^0.55.0",
    "@unocss/preset-uno": "^0.55.0",
    "@unocss/reset": "^0.55.0",
    "@unocss/transformer-directives": "^0.55.0",
    "@unocss/transformer-variant-group": "^0.55.0",
    "@unocss/vite": "^0.55.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "hastscript": "^8.0.0",
    "jsdom": "^22.1.0",
    "lightningcss": "^1.21.5",
    "mdsvex": "^0.11.0",
    "prettier": "^3.0.1",
    "prettier-plugin-svelte": "^3.0.3",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-external-links": "^2.1.0",
    "rehype-slug": "^5.1.0",
    "remark-emoji": "^4.0.0",
    "remark-gemoji": "^7.0.1",
    "remark-toc": "^8.0.1",
    "svelte": "^4.2.0",
    "svelte-check": "^3.5.0",
    "svelte-eslint-parser": "^0.32.2",
    "svelte-preprocess-import-assets": "^1.0.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-imagetools": "^5.0.7",
    "vitest": "^0.34.1"
  },
  "dependencies": {
    "@fontsource/noto-sans-sc": "^5.0.7",
    "@fontsource/rubik": "^5.0.8",
    "dayjs": "^1.11.9",
    "shiki": "^0.14.3"
  }
}
