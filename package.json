{
  "name": "sveltekit-template",
  "version": "0.0.2",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build:site": "vite build",
    "build:lib": "svelte-kit sync && svelte-package",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "eslint . --fix",
    "format": "prettier --write ./**/*.{js,cjs,ts,html,css,svelte,md,json}"
  },
  "devDependencies": {
    "@iconify-json/icon-park-outline": "^1.1.15",
    "@iconify-json/simple-icons": "^1.1.85",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.6",
    "@sveltejs/package": "^2.2.4",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@unocss/core": "^0.58.0",
    "@unocss/eslint-config": "^0.58.0",
    "@unocss/extractor-svelte": "^0.58.0",
    "@unocss/preset-attributify": "^0.58.0",
    "@unocss/preset-icons": "^0.58.0",
    "@unocss/preset-uno": "^0.58.0",
    "@unocss/reset": "^0.58.0",
    "@unocss/transformer-directives": "^0.58.0",
    "@unocss/transformer-variant-group": "^0.58.0",
    "@unocss/vite": "^0.58.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-svelte": "^2.35.1",
    "hastscript": "^8.0.0",
    "jsdom": "^23.0.1",
    "lightningcss": "^1.22.1",
    "mdsvex": "^0.11.0",
    "postcss-nested": "^6.0.1",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-external-links": "^3.0.0",
    "rehype-katex": "^7.0.0",
    "rehype-slug": "^6.0.0",
    "remark-emoji": "^4.0.1",
    "remark-gemoji": "^8.0.0",
    "remark-math": "^6.0.0",
    "remark-toc": "^9.0.0",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.2",
    "svelte-eslint-parser": "^0.33.1",
    "svelte-preprocess-import-assets": "^1.1.0",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vite-imagetools": "^6.2.8",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "@fontsource/noto-sans-sc": "^5.0.17",
    "@fontsource/rubik": "^5.0.18",
    "dayjs": "^1.11.10",
    "shiki": "^0.14.7"
